{
  "package": {
    "name": "tw-maven-central",
    "repo": "tw",
    "subject": "kpritam",
    "desc": "This is a private maven repo.",
    "website_url": "http://www.thoughtworks.com",
    "issue_tracker_url": "https://github.com/kpritam/travis-ci-sample/issues",
    "vcs_url": "https://github.com/kpritam/travis-ci-sample.git",
    "github_use_tag_release_notes": true,
    "licenses": ["Unlicense"],
    "public_download_numbers": false,
    "public_stats": false
  },

  "version": {
    "name": "${project.version}",
    "vcs_tag": "${project.version}",
    "released": "${build.timestamp}"
  },

  "files":
  [
    {"includePattern": "csw-location/target/scala-2.12/(.*\\.jar))", "excludePattern": ".*/do-not-deploy/.*", "uploadPattern": ".0.1-SNAPSHOT/$1"}
  ],

  "publish": true
}